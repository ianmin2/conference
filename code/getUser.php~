		<script type="text/javascript">

			$(function(){
				$(".top-navbar").toggleClass("toggle");
				$(".sidebar-left").toggleClass("toggle");
				$(".page-content").toggleClass("toggle");
				s = $("#search");
				s.keyup(function(){
					
					_v = s.val();

					$.ajax({  
					  type: "POST",  
					  url: $__url+ "timeline.php",  
					  data: { term : _v },
					  dataType:'jsonp', 
					  success: function(data) {
						 $("#content").html(data);	
						 //console.log(data)					 
					  }  
							
					});  
					
				})
				
			});

		</script>
		
		
